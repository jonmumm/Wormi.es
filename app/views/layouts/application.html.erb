<!DOCTYPE html>
<html>
<head>
  <title>Wormies</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= yield :head %>
  <%= csrf_meta_tags %>
</head>
<body>


  <div class="wrapper">

    <div class="header">
      <a href="/">
        <%= image_tag "wormies.png" %>
      </a>
      <h2>Face to face online book clubs.</h2>
    </div>

    <div class="container">
      <%= yield %>
    </div>

    <div class="push"></div>

  </div>

  <div class="footer">
    <div class="container">
      <%= image_tag "wormies.png" %>
      <span class="email">
        email us | <a href="mailto:contact@wormi.es">contact@wormi.es</a>
      </span>
    </div>
  </div>

</body>
</html>
